{
  "name": "duyn-api",
  "version": "1.0.0",
  "description": "My Assistant",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit"
  },
  "_moduleAliases": {
    "@infrastructure": "server/infrastructure",
    "@services": "server/services",
    "@domain": "server/domain",
    "@adapters": "server/adapters",
    "@ioc": "server/infrastructure/IoC"
  },
  "dependencies": {
    "@google-cloud/speech": "^4.1.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.9.5",
    "glob": "^7.1.6",
    "inversify": "^5.0.5",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.9",
    "pino": "^6.8.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^3.0.4",
    "socket.io-stream": "^0.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.0",
    "@types/mongoose": "^5.10.3",
    "@types/pino": "^6.3.4",
    "@types/socket.io": "^2.1.12",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "author": "Gialynguyen <gialynguyen@gmail.com> (https://github.com/gialynguyen)"
}
